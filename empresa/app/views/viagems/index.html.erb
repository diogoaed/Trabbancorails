<h1>Listing viagems</h1>

<table>
  <tr>
    <th>Numeroviagem</th>
    <th>Data</th>
    <th>Obs</th>
    <th>Kmini</th>
    <th>Kmfim</th>
    <th>Codigoveiculo</th>
    <th>Codigofuncionario</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @viagems.each do |viagem| %>
  <tr>
    <td><%= viagem.Numeroviagem %></td>
    <td><%= viagem.Data %></td>
    <td><%= viagem.Obs %></td>
    <td><%= viagem.Kmini %></td>
    <td><%= viagem.Kmfim %></td>
    <td><%= viagem.CodigoVeiculo %></td>
    <td><%= viagem.Codigofuncionario %></td>
    <td><%= link_to 'Show', viagem %></td>
    <td><%= link_to 'Edit', edit_viagem_path(viagem) %></td>
    <td><%= link_to 'Destroy', viagem, :method => :delete, :data => { :confirm => 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Viagem', new_viagem_path %>
